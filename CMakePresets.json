{
  "version": 6,
  "configurePresets": [
    {
      "name": "Debug",
      "binaryDir": "${sourceDir}/Build/Debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "Development",
      "binaryDir": "${sourceDir}/Build/Development",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo"
      }
    },
    {
      "name": "Release",
      "binaryDir": "${sourceDir}/Build/Release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    }
  ]
}